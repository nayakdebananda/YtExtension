{
	"name": "youtube-downloader",
	"displayName": "YouTube Downloader",
	"description": "This extension will help you to download youtube video",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:youtube-downloader.YouTubeVideoDownload"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "youtube-downloader.YouTubeVideoDownload",
				"title": "YouTube Downloader"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"ytdl-core": "^4.9.1"
	}
}
